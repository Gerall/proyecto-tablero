<%- model_class = Member -%>
<div class="page-header">
  <h1>Listado de members de: <%= @team.name %></h1>
</div>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>nombre</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
     <%= render @members %>
  </tbody>
  <tfoot>
    <tr>
      <td><%= link_to 'Nuevo members', new_team_member_path, :remote => true  %> </td>
      <td><%= link_to 'Atras', teams_path %></td>
    </tr>
  </tfoot>
</table>

